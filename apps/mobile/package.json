  "scripts": {
    "dev": "expo start",
    "dev:tunnel": "expo start --tunnel -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:reverse": "adb start-server && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:19000 tcp:19000 && adb reverse tcp:19001 tcp:19001",
    "android:dev": "EXPO_USE_POLLING=1 CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=100 expo start --tunnel --android",
    "android:clean": "adb reverse --remove-all || true && rm -rf .expo .expo-shared && EXPO_USE_POLLING=1 CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=100 expo start -c --tunnel --android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
